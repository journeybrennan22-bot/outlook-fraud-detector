<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
<Id>b2c3d4e5-f6a7-8901-bcde-123456789abc</Id>
<Version>1.0.1.0</Version>
<ProviderName>Pure Logic Escrow</ProviderName>
<DefaultLocale>en-US</DefaultLocale>
<DisplayName DefaultValue="Email Fraud Detector"/>
<Description DefaultValue="Detects email fraud including lookalike domains, wire fraud keywords, and impersonation attempts."/>
<IconUrl DefaultValue="https://journeybrennan22-bot.github.io/outlook-fraud-detector/assets/icon-64.png"/>
<HighResolutionIconUrl DefaultValue="https://journeybrennan22-bot.github.io/outlook-fraud-detector/assets/icon-128.png"/>
<SupportUrl DefaultValue="https://journeybrennan22-bot.github.io/outlook-fraud-detector/"/>
<AppDomains>
<AppDomain>https://journeybrennan22-bot.github.io</AppDomain>
</AppDomains>
<Hosts>
<Host Name="Mailbox"/>
</Hosts>
<Requirements>
<Sets>
<Set Name="Mailbox" MinVersion="1.1"/>
</Sets>
</Requirements>
<FormSettings>
<Form xsi:type="ItemRead">
<DesktopSettings>
<SourceLocation DefaultValue="https://journeybrennan22-bot.github.io/outlook-fraud-detector/src/taskpane.html"/>
<RequestedHeight>250</RequestedHeight>
</DesktopSettings>
</Form>
</FormSettings>
<Permissions>ReadWriteMailbox</Permissions>
<Rule xsi:type="RuleCollection" Mode="Or">
<Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
</Rule>
<DisableEntityHighlighting>false</DisableEntityHighlighting>
<VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
<VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xsi:type="VersionOverridesV1_1">
<Requirements>
<bt:Sets DefaultMinVersion="1.5">
<bt:Set Name="Mailbox"/>
</bt:Sets>
</Requirements>
<Hosts>
<Host xsi:type="MailHost">
<DesktopFormFactor>
<ExtensionPoint xsi:type="MessageReadCommandSurface">
<OfficeTab id="TabDefault">
<Group id="FraudDetectorGroup">
<Label resid="GroupLabel"/>
<Control xsi:type="Button" id="FraudDetectorButton">
<Label resid="ButtonLabel"/>
<Supertip>
<Title resid="ButtonLabel"/>
<Description resid="ButtonDescription"/>
</Supertip>
<Icon>
<bt:Image size="16" resid="Icon.16x16"/>
<bt:Image size="32" resid="Icon.32x32"/>
<bt:Image size="80" resid="Icon.80x80"/>
</Icon>
<Action xsi:type="ShowTaskpane">
<SourceLocation resid="Taskpane.Url"/>
<SupportsPinning>true</SupportsPinning>
</Action>
</Control>
</Group>
</OfficeTab>
</ExtensionPoint>
</DesktopFormFactor>
</Host>
</Hosts>
<Resources>
<bt:Images>
<bt:Image id="Icon.16x16" DefaultValue="https://journeybrennan22-bot.github.io/outlook-fraud-detector/assets/icon-16.png"/>
<bt:Image id="Icon.32x32" DefaultValue="https://journeybrennan22-bot.github.io/outlook-fraud-detector/assets/icon-32.png"/>
<bt:Image id="Icon.80x80" DefaultValue="https://journeybrennan22-bot.github.io/outlook-fraud-detector/assets/icon-80.png"/>
</bt:Images>
<bt:Urls>
<bt:Url id="Taskpane.Url" DefaultValue="https://journeybrennan22-bot.github.io/outlook-fraud-detector/src/taskpane.html"/>
</bt:Urls>
<bt:ShortStrings>
<bt:String id="GroupLabel" DefaultValue="Email Security"/>
<bt:String id="ButtonLabel" DefaultValue="Fraud Detector"/>
</bt:ShortStrings>
<bt:LongStrings>
<bt:String id="ButtonDescription" DefaultValue="Analyzes the current email for signs of fraud including lookalike domains, impersonation, and wire fraud keywords."/>
</bt:LongStrings>
</Resources>
</VersionOverrides>
</VersionOverrides>
</OfficeApp>
